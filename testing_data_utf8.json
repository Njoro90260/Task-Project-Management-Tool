[{"model": "projects.feature", "pk": 1, "fields": {"title": "Task Management", "description": "Organize and manage tasks efficiently. Create tasks, assign them to team members, set priorities, and track progress until completion.", "svg_icon": "", "icon_file": "", "is_active": true, "created_at": "2025-01-24T12:49:08.145Z", "updated_at": "2025-01-24T12:49:08.145Z"}}, {"model": "projects.feature", "pk": 2, "fields": {"title": "Project Management", "description": "Plan, monitor, and execute projects from start to finish. Assign projects to users, set deadlines, and manage team collaboration to ensure timely delivery.", "svg_icon": "", "icon_file": "", "is_active": true, "created_at": "2025-01-24T12:49:31.238Z", "updated_at": "2025-01-24T12:49:31.238Z"}}, {"model": "projects.feature", "pk": 3, "fields": {"title": "Team Collaboration", "description": "Foster communication within your team. Comment on tasks, share files, and collaborate seamlessly across projects in real-time.", "svg_icon": "", "icon_file": "", "is_active": true, "created_at": "2025-01-24T12:49:49.001Z", "updated_at": "2025-01-24T12:49:49.001Z"}}, {"model": "projects.feature", "pk": 4, "fields": {"title": "Time Tracking", "description": "Log work hours for each task or project. Track how much time is spent on tasks and generate detailed reports for better resource management.", "svg_icon": "", "icon_file": "", "is_active": true, "created_at": "2025-01-24T12:50:09.884Z", "updated_at": "2025-01-24T12:50:09.884Z"}}, {"model": "projects.feature", "pk": 5, "fields": {"title": "Progress Tracking", "description": "Monitor the progress of tasks and projects using visual tools such as Gantt charts or Kanban boards. Stay updated on what's done, what's in progress, and what's yet to start.", "svg_icon": "", "icon_file": "", "is_active": true, "created_at": "2025-01-24T12:50:33.621Z", "updated_at": "2025-01-24T12:50:33.622Z"}}, {"model": "projects.feature", "pk": 6, "fields": {"title": "Notifications & Alerts", "description": "Stay informed with real-time notifications about task updates, due dates, and upcoming deadlines to ensure you never miss important events.", "svg_icon": "", "icon_file": "", "is_active": true, "created_at": "2025-01-24T12:50:55.833Z", "updated_at": "2025-01-24T12:50:55.833Z"}}, {"model": "projects.feature", "pk": 7, "fields": {"title": "Task Prioritization", "description": "Set task priorities (high, medium, low) to ensure the most important tasks are completed first. Efficiently manage resources and time.", "svg_icon": "", "icon_file": "", "is_active": true, "created_at": "2025-01-24T12:51:36.083Z", "updated_at": "2025-01-24T12:51:36.083Z"}}, {"model": "projects.feature", "pk": 8, "fields": {"title": "File Sharing", "description": "Upload and share documents, images, and other files within tasks and projects. Collaborate on files with your team directly from the platform.", "svg_icon": "", "icon_file": "", "is_active": true, "created_at": "2025-01-24T12:51:54.769Z", "updated_at": "2025-01-24T12:51:54.769Z"}}, {"model": "projects.feature", "pk": 9, "fields": {"title": "Search & Filters", "description": "Quickly find tasks, projects, or team members by searching for keywords or applying filters based on project status, priority, and due dates.", "svg_icon": "", "icon_file": "", "is_active": true, "created_at": "2025-01-24T12:52:15.260Z", "updated_at": "2025-01-24T12:52:15.260Z"}}, {"model": "projects.feature", "pk": 10, "fields": {"title": "Project Timeline", "description": "Visualize your projectÂ’s timeline and milestones. Track progress across phases and ensure all tasks are completed on time.", "svg_icon": "", "icon_file": "", "is_active": true, "created_at": "2025-01-24T12:52:39.237Z", "updated_at": "2025-01-24T12:52:39.237Z"}}, {"model": "projects.feature", "pk": 11, "fields": {"title": "Kanban Board", "description": "Use drag-and-drop Kanban boards to manage task statuses, organize workflow, and visualize project tasks in various stages (To Do, In Progress, Done).", "svg_icon": "", "icon_file": "", "is_active": true, "created_at": "2025-01-24T12:52:58.014Z", "updated_at": "2025-01-24T12:52:58.014Z"}}, {"model": "projects.feature", "pk": 12, "fields": {"title": "Reports & Analytics", "description": "Generate performance reports and detailed analytics on project and task statuses, team productivity, and project health.", "svg_icon": "", "icon_file": "", "is_active": true, "created_at": "2025-01-24T12:53:25.257Z", "updated_at": "2025-01-24T12:53:25.257Z"}}, {"model": "projects.feature", "pk": 13, "fields": {"title": "Role-based Permissions", "description": "Set specific roles for team members (e.g., Admin, Manager, Team Member) to control access to different parts of the project management system.", "svg_icon": "", "icon_file": "", "is_active": true, "created_at": "2025-01-24T12:53:46.873Z", "updated_at": "2025-01-24T12:53:46.873Z"}}, {"model": "projects.feature", "pk": 14, "fields": {"title": "Calendar Integration", "description": "Sync tasks and deadlines with Google Calendar or any other external calendar service. Ensure all important dates are integrated into your daily schedule.", "svg_icon": "", "icon_file": "", "is_active": true, "created_at": "2025-01-24T12:54:07.927Z", "updated_at": "2025-01-24T12:54:07.927Z"}}, {"model": "projects.feature", "pk": 15, "fields": {"title": "User Management", "description": "Manage your team members by adding or removing users, assigning them roles, and tracking their participation in various projects and tasks.", "svg_icon": "", "icon_file": "", "is_active": true, "created_at": "2025-01-24T12:54:30.754Z", "updated_at": "2025-01-24T12:54:30.754Z"}}, {"model": "projects.feature", "pk": 16, "fields": {"title": "Recurring Tasks", "description": "Set tasks to repeat automatically based on your needs (e.g., daily, weekly, monthly), ensuring regular activities are always completed on time.", "svg_icon": "", "icon_file": "", "is_active": true, "created_at": "2025-01-24T12:54:50.158Z", "updated_at": "2025-01-24T12:54:50.159Z"}}, {"model": "projects.feature", "pk": 17, "fields": {"title": "Customizable Dashboards", "description": "Personalize your dashboard to view the most relevant metrics and activities. Display key performance indicators (KPIs), project progress, and more.", "svg_icon": "", "icon_file": "", "is_active": true, "created_at": "2025-01-24T12:55:13.225Z", "updated_at": "2025-01-24T12:55:13.225Z"}}, {"model": "projects.feature", "pk": 18, "fields": {"title": "External Integrations", "description": "Integrate with third-party tools such as Slack, Google Drive, Trello, or GitHub to streamline workflows and enhance collaboration.", "svg_icon": "", "icon_file": "", "is_active": true, "created_at": "2025-01-24T12:55:33.799Z", "updated_at": "2025-01-24T12:55:33.799Z"}}, {"model": "admin.logentry", "pk": 1, "fields": {"action_time": "2025-01-24T12:49:08.157Z", "user": 2, "content_type": 11, "object_id": "1", "object_repr": "Task Management", "action_flag": 1, "change_message": "[{\"added\": {}}]"}}, {"model": "admin.logentry", "pk": 2, "fields": {"action_time": "2025-01-24T12:49:31.251Z", "user": 2, "content_type": 11, "object_id": "2", "object_repr": "Project Management", "action_flag": 1, "change_message": "[{\"added\": {}}]"}}, {"model": "admin.logentry", "pk": 3, "fields": {"action_time": "2025-01-24T12:49:49.040Z", "user": 2, "content_type": 11, "object_id": "3", "object_repr": "Team Collaboration", "action_flag": 1, "change_message": "[{\"added\": {}}]"}}, {"model": "admin.logentry", "pk": 4, "fields": {"action_time": "2025-01-24T12:50:09.900Z", "user": 2, "content_type": 11, "object_id": "4", "object_repr": "Time Tracking", "action_flag": 1, "change_message": "[{\"added\": {}}]"}}, {"model": "admin.logentry", "pk": 5, "fields": {"action_time": "2025-01-24T12:50:33.627Z", "user": 2, "content_type": 11, "object_id": "5", "object_repr": "Progress Tracking", "action_flag": 1, "change_message": "[{\"added\": {}}]"}}, {"model": "admi